    <!-- https://liidr.org/saref-ta/accomplishes -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/accomplishes">
        <owl:inverseOf rdf:resource="https://liidr.org/saref-ta/isAccomplishedBy"/>
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/Device"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Task"/>
        <rdfs:comment xml:lang="en">A relationship between a certain entity (e.g., a device) and the task it accomplishes</rdfs:comment>
        <rdfs:label xml:lang="en">accomplishes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/actsUpon -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/actsUpon">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/Command"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/State"/>
        <rdfs:comment xml:lang="en">A relationship between a command and a state</rdfs:comment>
        <rdfs:label xml:lang="en">acts upon</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/composesNetworkPacket -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/composesNetworkPacket">
        <owl:inverseOf rdf:resource="https://liidr.org/saref-ta/isComposedByCommand"/>
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/NetworkProtocolCommand"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/NetworkPacket"/>
        <rdfs:comment xml:lang="en">A network command composes a network packet.</rdfs:comment>
        <rdfs:label xml:lang="en">compose network packet</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/consistsOf -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/consistsOf">
        <rdfs:comment xml:lang="en">A relationship indicating a composite entity that consists of other entities (e.g., a temperature/humidity sensor that consists of a temperature sensor and a humidity sensor)</rdfs:comment>
        <rdfs:label xml:lang="en">consists of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/controlsProperty -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/controlsProperty">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/Device"/>
        <rdfs:comment xml:lang="en">A relationship specifying the property that can be controlled by a certain device</rdfs:comment>
        <rdfs:label xml:lang="en">controls property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasCommand -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasCommand">
        <owl:inverseOf rdf:resource="https://liidr.org/saref-ta/isCommandOf"/>
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/Function"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Command"/>
        <rdfs:comment xml:lang="en">A relationship between a function and a command</rdfs:comment>
        <rdfs:label xml:lang="en">has command</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasFunction -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasFunction">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/Device"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Function"/>
        <rdfs:comment xml:lang="en">A relationship identifying the function of a device</rdfs:comment>
        <rdfs:label xml:lang="en">has function</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasMeasurement -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasMeasurement">
        <owl:inverseOf rdf:resource="https://liidr.org/saref-ta/isMeasurementOf"/>
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/FeatureOfInterest"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Measurement"/>
        <rdfs:comment xml:lang="en">A relationship between a feature of interest and a measurement about it</rdfs:comment>
        <rdfs:label xml:lang="en">has measurement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasMeterReading -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasMeterReading">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/MeteringFunction"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Measurement"/>
        <rdfs:comment xml:lang="en">A relationship between a metering function and the measurement of the reading</rdfs:comment>
        <rdfs:label xml:lang="en">has meter reading</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasMeterReadingType -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasMeterReadingType">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/MeteringFunction"/>
        <rdfs:comment xml:lang="en">A relationship identifying the reading type of a metering function (e.g., Water, Gas, Pressure , Energy , Power, etc.)</rdfs:comment>
        <rdfs:label xml:lang="en">has meter reading type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasNetworkPacketProperty -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasNetworkPacketProperty">
        <owl:inverseOf rdf:resource="https://liidr.org/saref-ta/isPropertyOfNetworkPacket"/>
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/NetworkPacket"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/NetworkPacketProperty"/>
        <rdfs:comment xml:lang="en">A network packet has a network packet property</rdfs:comment>
        <rdfs:label xml:lang="en">has network packet property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasPreferredSettingsBy -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasPreferredSettingsBy">
        <owl:inverseOf rdf:resource="https://liidr.org/saref-ta/prefersSettings"/>
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/TrustIndicator"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Profile"/>
        <rdfs:comment>The trust indictor is subject to the preferred settings of the profile.</rdfs:comment>
        <rdfs:label>has preferred settings by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasPrice -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasPrice">
        <rdfs:comment xml:lang="en">A relationship indentifying the price associated to an entity</rdfs:comment>
        <rdfs:label xml:lang="en">has price</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasProfile -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasProfile">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/Device"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Profile"/>
        <rdfs:comment xml:lang="en">A relationship associating a profile to a certain device</rdfs:comment>
        <rdfs:label xml:lang="en">has profile</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasProperty -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasProperty">
        <owl:inverseOf rdf:resource="https://liidr.org/saref-ta/isPropertyOf"/>
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/FeatureOfInterest"/>
        <rdfs:comment xml:lang="en">A relationship between a feature of interest and a property of that feature</rdfs:comment>
        <rdfs:label xml:lang="en">has property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasSensingRange -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasSensingRange">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/SensingFunction"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Measurement"/>
        <rdfs:comment xml:lang="en">A relationship between a sensing function and a measurement identifying the range of a sensor detection</rdfs:comment>
        <rdfs:label xml:lang="en">has sensing range</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasSensorType -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasSensorType">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/SensingFunction"/>
        <rdfs:comment xml:lang="en">A relationship identifying the sensing type of a sensor detection (i.e., Temperature, Occupancy, Humidity, Motion , Smoke, Pressure, etc.) </rdfs:comment>
        <rdfs:label xml:lang="en">has sensor type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasServiceProperty -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasServiceProperty">
        <owl:inverseOf rdf:resource="https://liidr.org/saref-ta/isServicePropertyOf"/>
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/Service"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/ServiceProperty"/>
        <rdfs:comment xml:lang="en">A service has a service property.</rdfs:comment>
        <rdfs:label xml:lang="en">has service property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasState -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasState">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/Device"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/State"/>
        <rdfs:comment xml:lang="en">A relationship identifying the type of state of a device</rdfs:comment>
        <rdfs:label xml:lang="en">has state</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasThresholdMeasurement -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasThresholdMeasurement">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/EventFunction"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Measurement"/>
        <rdfs:comment xml:lang="en">A relationship associated with an event function to notify that a certain threshold measurement has been exceeded</rdfs:comment>
        <rdfs:label xml:lang="en">has threshold measurement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasTime -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasTime">
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Time"/>
        <rdfs:comment xml:lang="en">A relationship to associate time information to an entity</rdfs:comment>
        <rdfs:label xml:lang="en">has time</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasTrustIndicator -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasTrustIndicator">
        <owl:inverseOf rdf:resource="https://liidr.org/saref-ta/isTrustIndicatorOf"/>
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/Task"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/TrustIndicator"/>
        <rdfs:comment>A task has a trust indicator</rdfs:comment>
        <rdfs:label>has trust indicator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/hasTypicalConsumption -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/hasTypicalConsumption">
        <rdfs:comment xml:lang="en">A relationship identifying the typical (energy or power) consumption of a device</rdfs:comment>
        <rdfs:label xml:lang="en">has typical consumption</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/isAbout -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/isAbout">
        <rdfs:comment xml:lang="en">A relationship identifying what an entity, such as a profile, is about</rdfs:comment>
        <rdfs:label xml:lang="en">isAbout</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/isAccomplishedBy -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/isAccomplishedBy">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/Task"/>
        <rdfs:comment xml:lang="en">A relationship identifying an entity (e.g., a device) that can accomplish a task.</rdfs:comment>
        <rdfs:label xml:lang="en">is accomplished by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/isCommandOf -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/isCommandOf">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/Command"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Function"/>
        <rdfs:comment xml:lang="en">A relationship between a command and a function.</rdfs:comment>
        <rdfs:label xml:lang="en">is command of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/isComposedByCommand -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/isComposedByCommand">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/NetworkPacket"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/NetworkProtocolCommand"/>
        <rdfs:comment xml:lang="en">A network packet is composed by a network command</rdfs:comment>
        <rdfs:label xml:lang="en">is composed by command</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/isControlledByDevice -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/isControlledByDevice">
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Device"/>
        <rdfs:comment xml:lang="en">A relationship specifying the devices that can control a certain property</rdfs:comment>
        <rdfs:label xml:lang="en">is controlled by device</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/isMeasuredByDevice -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/isMeasuredByDevice">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/Measurement"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Device"/>
        <rdfs:comment xml:lang="en">A relationship specifying the devices that can measure a certain property</rdfs:comment>
        <rdfs:label xml:lang="en">is measured by device</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/isMeasuredIn -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/isMeasuredIn">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/Measurement"/>
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/TrustIndicator"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/UnitOfMeasure"/>
        <rdfs:comment xml:lang="en">A relationship identifying the unit of measure used for a certain entity.</rdfs:comment>
        <rdfs:label xml:lang="en">is measured in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/isMeasurementOf -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/isMeasurementOf">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/Measurement"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/FeatureOfInterest"/>
        <rdfs:comment xml:lang="en">A relationship between a measurement and the feature of interest whose quality was measured</rdfs:comment>
        <rdfs:label xml:lang="en">isMeasurementOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/isObservedBy -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/isObservedBy">
        <owl:inverseOf rdf:resource="https://liidr.org/saref-ta/observesFeatureOfInterest"/>
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/FeatureOfInterest"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/TrustIndicator"/>
        <rdfs:comment>The feature of interest is observed by an trust indicator.</rdfs:comment>
        <rdfs:label>is observed by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/isOfferedBy -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/isOfferedBy">
        <owl:inverseOf rdf:resource="https://liidr.org/saref-ta/offers"/>
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/Service"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Device"/>
        <rdfs:comment xml:lang="en">A relationship between a service and a device that offers the service</rdfs:comment>
        <rdfs:label xml:lang="en">is offered by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/isPropertyOf -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/isPropertyOf">
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/FeatureOfInterest"/>
        <rdfs:comment xml:lang="en">A relationship between a property and the feature of interest it belongs to</rdfs:comment>
        <rdfs:label xml:lang="en">is property of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/isPropertyOfNetworkPacket -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/isPropertyOfNetworkPacket">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/NetworkPacketProperty"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/NetworkPacket"/>
        <rdfs:comment>The network property is a property of the network packet.</rdfs:comment>
        <rdfs:label xml:lang="en">is property of network packet</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/isServicePropertyOf -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/isServicePropertyOf">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/ServiceProperty"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Service"/>
        <rdfs:comment xml:lang="en">A property is a service property of a service.</rdfs:comment>
        <rdfs:label xml:lang="en">is service property of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/isTrustIndicatorOf -->

    <owl:ObjectProperty rdf:about="https://liidr.org/saref-ta/isTrustIndicatorOf">
        <rdfs:domain rdf:resource="https://liidr.org/saref-ta/TrustIndicator"/>
        <rdfs:range rdf:resource="https://liidr.org/saref-ta/Task"/>
        <rdfs:comment>The trust indicator is related to the task.</rdfs:comment>
        <rdfs:label>is trust indicator of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://liidr.org/saref-ta/isTrustMeasuredBy -->

