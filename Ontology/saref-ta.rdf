        <dcterms:title>Smart Applications Reference Ontology for Trust Agents</dcterms:title>
        <vann:preferredNamespacePrefix>saref-ta</vann:preferredNamespacePrefix>
        <rdfs:comment xml:lang="en">The Smart Application Reference Ontology for Trust Agents (SAREF-TA) intends to describe functions and properties of devices used for trust-related recommendations to enable trustworthy collaboration between devices managed by different organisations.</rdfs:comment>
        <rdfs:label xml:lang="en">SAREF-TA: The Smart Applications Reference Ontology for Trust Agents</rdfs:label>
        <rdfs:seeAlso>saref-ta</rdfs:seeAlso>
        <owl:versionInfo xml:lang="en">The Smart Application Reference Ontology for Trust Agents (SAREF-TA) intends to describe functions and properties of devices used for trust-related recommendations to enable trustworthy collaboration between devices managed by different organisations.</owl:versionInfo>
        <owl:versionInfo xml:lang="en">v1.0.0</owl:versionInfo>
    </owl:Ontology>
    


